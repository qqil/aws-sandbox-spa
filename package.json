{
  "name": "shop-spa",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .gitignore ./",
    "lint:fix": "eslint --ignore-path .gitignore  --fix ./",
    "build": "tsc && vite build",
    "build:deploy": "npm run build && npm run finch:deploy",
    "build:deploy:invalidate": "npm run build:deploy && npm run cloudfront:invalidateCache",
    "finch:deploy": "sls client deploy --no-config-change --no-policy-change --no-cors-change --no-confirm",
    "finch:remove": "sls client remove",
    "cloudformation:deploy": "sls deploy",
    "cloudformation:remove": "sls remove",
    "cloudfront:domainInfo": "sls domainInfo",
    "cloudfront:invalidateCache": "sls invalidateCloudFrontCache"
  },
  "dependencies": {
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.1.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "serverless": "^3.22.0",
    "serverless-finch": "^4.0.0",
    "serverless-s3-cleaner": "^2.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0"
  }
}
